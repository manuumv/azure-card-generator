{
  "rootDir": "../../",
  "preset": "ts-jest",
  "setupFiles": [
    "<rootDir>/config/test/setupTest.ts",
    "jest-localstorage-mock"
  ],
  "restoreMocks": true,
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "automock": false,
  "moduleNameMapper": {
    "^styles(.*)$": "<rootDir>/src/styles$1",
    "^common(.*)$": "<rootDir>/src/common$1",
    "^api(.*)$": "<rootDir>/src/api$1"
  }
}
